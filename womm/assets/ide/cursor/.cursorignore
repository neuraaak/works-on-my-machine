# Python virtual environments
.venv/
venv/
env/
ENV/
env.bak/
venv.bak/
.conda/
__pypackages__/

# Node.js dependencies
node_modules/
.npm/
.yarn/
.pnp/
.pnp.js
jspm_packages/

# Python package metadata
*.egg-info/
.eggs/
*.egg
.installed.cfg

